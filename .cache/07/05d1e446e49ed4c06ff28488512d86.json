ine":44,"column":46}},{"generated":{"line":100,"column":50},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":44,"column":13}},{"name":"header","generated":{"line":100,"column":53},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":44,"column":51}},{"generated":{"line":100,"column":59},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":44,"column":6}},{"generated":{"line":101,"column":0},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":45,"column":5}},{"generated":{"line":102,"column":0},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":46,"column":3}},{"name":"View","generated":{"line":106,"column":2},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":3,"column":34}},{"generated":{"line":108,"column":15},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":49,"column":15}},{"name":"CategPaginationView","generated":{"line":108,"column":19},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":49,"column":19}},{"generated":{"line":108,"column":38},"source":"src/js/views/pagination/paginationCategories.js","original":{"line":49,"column":15}}],"sources":{"src/js/views/pagination/paginationCategories.js":"import View from '../View.js';\r\n\r\nclass CategPaginationView extends View {\r\n  _parentElement = document.querySelector('.header');\r\n\r\n  addHandlerClick(handler) {\r\n    this._parentElement.addEventListener('click', function (e) {\r\n      const btn = e.target.closest('.fa');\r\n      if (!btn) return;\r\n\r\n      handler(btn.classList[1], 'categ');\r\n    });\r\n  }\r\n\r\n  _generateMarkup() {\r\n    this._clear();\r\n    const header = `<span class=\"text\">\r\n                    <h2 class=\"header-text\">Categories</h2>\r\n                    </span>`;\r\n    const curPage = this._data.page;\r\n    const numPages = Math.ceil(\r\n      this._data.results.length / this._data.resultsPerPage\r\n    );\r\n    const rendNextButton = function () {\r\n      return `<i class=\"fa fa-arrow-right\" aria-hidden=\"true\"></i>`;\r\n    };\r\n    const rendPrevButton = function () {\r\n      return `<i class=\"fa fa-arrow-left\" aria-hidden=\"true\"></i>`;\r\n    };\r\n    // Page 1, and there are other pages\r\n    if (curPage === 1 && numPages > 1) {\r\n      return header + rendNextButton();\r\n    }\r\n    // Page 1, and there are no other pages\r\n    if (curPage === 1 && numPages === 1) {\r\n      return header;\r\n    }\r\n    // Last page\r\n    if (curPage === numPages) {\r\n      return rendPrevButton() + header;\r\n    }\r\n    // Other page\r\n    if (curPage < numPages) {\r\n      return rendNextButton() + rendPrevButton() + header;\r\n    }\r\n  }\r\n}\r\n\r\nexport default new CategPaginationView();\r\n"},"lineCount":null}},"error":null,"hash":"739f862e7b19cc6f316b4c581a16619a","cacheData":{"env":{}}}